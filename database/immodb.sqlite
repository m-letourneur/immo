CREATE TABLE users(
	id INT PRIMARY KEY NOT NULL, 
	first_name TEXT NOT NULL,
	last_name TEXT NOT NULL,
	birth_date DATE NOT NULL
	);

CREATE TABLE estates(
	id INT PRIMARY KEY NOT NULL,
	creator_uuid INT NOT NULL,
	city TEXT NOT NULL,
	nb_rooms INT NOT NULL,
	description TEXT,
	rooms_description TEXT,
	owner TEXT
	);

.mode csv
.separator ";"
.headers off
.save immodb.db
.quit

