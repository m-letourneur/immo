CREATE TABLE users(
	uuid INT PRIMARY KEY NOT NULL, 
	first_name TEXT NOT NULL,
	last_name TEXT NOT NULL,
	birth_date DATE NOT NULL
	);

CREATE TABLE estates(
	uuid INT PRIMARY KEY NOT NULL,
	creator_uuid INT NOT NULL,
	description TEXT,
	city TEXT NOT NULL,
	nb_rooms INT,
	rooms_description TEXT,
	owner TEXT
	);

